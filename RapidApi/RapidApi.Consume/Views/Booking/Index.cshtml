@model List<RapidApi.Consume.Models.BookingApiViewModel.Result>

@{
    Layout = null;
    int count = 0;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br /><br />
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Hotel Name</th>
            <th>Hotel Rank</th>
            <th>Hotel Detail</th>
        </tr>
        @foreach (var item in Model)
        {
            count++;
            <tr>
                <th>@count</th>
                <td>@item.hotel_name</td>
                <td>@item.review_score</td>
                <td><a href="#" class="btn btn-outline-dark">Details</a></td>
            </tr>
        }
    </table>
</div>